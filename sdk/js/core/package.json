{
    "name": "bodhveda",
    "description": "JavaScript SDK for Bodhveda",
    "version": "0.0.4",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MudgalLabs/bodhveda.git"
    },
    "keywords": [
        "bodhveda",
        "notification",
        "sdk"
    ],
    "publishConfig": {
        "access": "public"
    },
    "bugs": {
        "url": "https://github.com/MudgalLabs/bodhveda/issues"
    },
    "homepage": "https://github.com/MudgalLabs/bodhveda#readme",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "npm run build:cjs && npm run build:esm",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "build:esm": "tsc -p tsconfig.esm.json",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build"
    },
    "dependencies": {
        "axios": "^1.11.0"
    },
    "devDependencies": {
        "typescript": "^5.9.2"
    }
}
