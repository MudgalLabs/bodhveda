{
    "name": "@bodhveda/react",
    "version": "0.0.4",
    "description": "React SDK for Bodhveda",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MudgalLabs/bodhveda.git"
    },
